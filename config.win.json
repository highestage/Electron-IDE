{
    "name": "electron-app",
    "executable_name": "electronapp",
    "product_name": "Electron App",
    "description": "Electron App",
    "author": {
        "name": "Your Name",
        "copyright": "Copyright (c) 2017 Your Name"
    },
    "dir": {
        "source": "./path/to/src",
        "dest": "./path/to/dest"
    },
    "verbose": true,
    "building_options": {
        "overwrite": true,
        "asar": true,
        "universal_binary": true,
        "js_obfuscate": true,
        "jsFileList": [
            "./path/to/src/js/file1.js",
            "./path/to/src/js/file2.js"
        ],
        "electron_version": "17.0.1",
        "platform": "win32",
        "archs": [
            "x64"
        ],
        "remove_localizations": [
            "am", "ar", "bg", "bn", "ca", "cs", "da", "el", "es_419", "et", "fa", "fi", "fil", "gu" ,"he", "hi", "hr", "hu", "id", "it", "kn", "ko", "lt", "lv", "ml", "mr", "ms", "nb", "nl", "pl", "pt_BR", "pt_PT", "ro", "sk", "sl", "sr", "sv", "sw", "ta", "te", "th", "tr", "uk", "vi"
        ],
        "icon": "./assets/icon.ico",
        "no_deref_symlinks": true,
        "no_junk": false,
        "no_prune": false,
        "temp": {
            "enabled": false,
            "dir": "./path/to/temp"
        },
        "extra_resource": [
            "./path/to/extra/resource"
        ],
        "ignore": [
            "./path/to/ignore/file"
        ]
    },
    "signing": {
        "enabled": true,
        "options": {
            "app_id": "com.electron.app",
            "version": "0.0.1",
            "build_version": "0.0.1",
            "publisher_display_name": "Your Name",
            "appx_icon_resources": "./path/to/icons"
        }
    },
    "packaging": {
        "open_packaged_app_location": true
    },
    "win32_metadata": {
        "company_name": "Your Company Name",
        "file_description": "Electron App",
        "original_filename": "electron-app",
        "product_name": "Electron App",
        "internal_name": "electron-app",
        "requested-execution-level": "asInvoker",
        "application-manifest": "./path/to/manifest.xml"
    }
}